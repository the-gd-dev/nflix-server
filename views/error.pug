doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        link(rel="shortcut icon" href="static/logo.png")
        title Error Page - Netflix Clone
        style.
            body {
                font-family: 'Calibri', sans-serif;
                padding : 0;
                margin :0;
                background:#101010;
            }
            main {
                padding : 18px;
            }
            .error-container  {
                display : flex;
                justify-content:center;
            }
            .error-wrapper {
                max-width : 500px;
            }
            .error-code  {
                text-align:center;
                color : #e9292c;
                font-size:8rem;
                font-weight : bold;
            }
            .error-message  {
                text-align:center;
                font-size:2rem;
                color : #f7f7f7;
            }
            .navbar {
                display:flex;
                font-family: Calibri, sans-serif;
                padding: 10px 0;
                align-items:center;
                justify-content:space-between;
                font-weight: 900;
            }
            footer {
                width:100%;
                margin :auto;
                margin-top:2rem;
            }
            footer h2 ,
            footer h1 {
                color:#e9292c;
                margin:0;
                padding:0;
                font-weight:600;
                text-align:center;
            }
    body
        div.navbar
            div
                img(src=`data:image/png;base64,`, alt="Netflix Logo" height='60')
        main 
            div.error-container  
                div.error-wrapper  
                    div.error-code 500
                    div.error-message Sorry, This page is not available for web.
        footer
            h1 Netflix Clone
            h2 &copy; 2022

